{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#a71cb9",
    "titleBar.activeForeground": "#ffffff"
  },
  "editor.autoClosingQuotes": "always",
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "cSpell.words": [
    "ngrx",
    "injectable",
    "validators",
    "datepicker",
    "theming",
    "snackbar",
    "truthy",
    "sidenav",
    "paginator",
    "toolbar",
    "unsubscribe"
  ],
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "**/tslint.json"
  ]
}
