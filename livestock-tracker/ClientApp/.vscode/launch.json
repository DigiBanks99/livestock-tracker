{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Debug HTTP Edge [localhost]",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Debug HTTPS Edge [localhost]",
      "url": "https://localhost:5001",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Chrome",
      "port": 5000,
      "webRoot": "${workspaceFolder}",
      "smartStep": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug HTTP [localhost]",
      "url": "http://localhost:5000",
      "webRoot": "${workspaceFolder}",
      "smartStep": true
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug HTTPS [localhost]",
      "url": "https://localhost:5001",
      "webRoot": "${workspaceFolder}",
      "smartStep": true
    }
  ]
}
